{
  "name": "United States Postal Service",
  "courier_code": "usps",
  "tracking_numbers": [
    {
      "name": "USPS 20",
      "id": "usps_20",
      "description": "20 digit USPS numbers",
      "tracking_url": "https://tools.usps.com/go/TrackConfirmAction?tLabels=%s",
      "regex": [
        "\\s*(?<SerialNumber>",
        "(?<ServiceType>([0-9]\\s*){2})",
        "(?<ShipperId>([0-9]\\s*){9})",
        "(?<PackageId>([0-9]\\s*){8})",
        ")",
        "(?<CheckDigit>[0-9]\\s*)"
      ],
      "validation": {
        "checksum": {
          "name": "mod10",
          "evens_multiplier": 3,
          "odds_multiplier": 1
        }
      },
      "test_numbers": {
        "valid": [
          "0307 1790 0005 2348 3741",
          " 0 3 0 7   1 7 9 0   0 0 0 5   2 3 4 8   3 7 4 1 ",
          "7112 3456 7891 2345 6787"
        ],
        "invalid": [
          "0307 1790 0005 2348 3742"
        ]
      },
      "additional": [
        {
          "name": "Service Type",
          "regex_group_name": "ServiceType",
          "lookup": [
            {
              "matches": "71",
              "name": "Certified Mail"
            },
            {
              "matches": "73",
              "name": "Insured Mail"
            },
            {
              "matches": "77",
              "name": "Registered Mail"
            },
            {
              "matches": "81",
              "name": "Return Receipt For Merchanise"
            }
          ]
        }
      ]
    },
